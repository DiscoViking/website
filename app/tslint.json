{
    "extends": [
        "tslint:latest",
        "tslint-react"
    ],
    "rules": {
        "jsx-no-lambda": false,
        "no-submodule-imports": [
            true,
            "@material-ui/core",
            "@material-ui/icons"
        ],
        "object-literal-key-quotes": [
            true,
            "as-needed"
        ]
    }
}